     <div class="CodeMirror cm-s-jupyter">
<div class=" highlight hl-ipython3"><pre><span></span><span class="n">MAX_SUMMARY_LEN</span> <span class="o">=</span> <span class="mi">10</span>
<span class="n">MAX_TEXT_LEN</span> <span class="o">=</span> <span class="mi">50</span>


<span class="c1"># make the texts and summaries arrays</span>
<span class="n">cleaned_text</span> <span class="o">=</span> <span class="n">np</span><span class="o">.</span><span class="n">array</span><span class="p">(</span><span class="n">dataframe</span><span class="p">[</span><span class="s2">"text"</span><span class="p">])</span>
<span class="n">cleaned_summary</span> <span class="o">=</span> <span class="n">np</span><span class="o">.</span><span class="n">array</span><span class="p">(</span><span class="n">dataframe</span><span class="p">[</span><span class="s2">"summary"</span><span class="p">])</span>

<span class="c1"># define lists for short text and short summaries</span>
<span class="n">short_text</span> <span class="o">=</span> <span class="p">[]</span>
<span class="n">short_summary</span> <span class="o">=</span> <span class="p">[]</span>

<span class="c1"># clean outliers</span>
<span class="k">for</span> <span class="n">i</span> <span class="ow">in</span> <span class="nb">range</span><span class="p">(</span><span class="nb">len</span><span class="p">(</span><span class="n">cleaned_text</span><span class="p">)):</span>
    <span class="k">if</span><span class="p">(</span><span class="nb">len</span><span class="p">(</span><span class="n">cleaned_summary</span><span class="p">[</span><span class="n">i</span><span class="p">]</span><span class="o">.</span><span class="n">split</span><span class="p">())</span> <span class="o">&lt;=</span> <span class="n">MAX_SUMMARY_LEN</span> <span class="ow">and</span> <span class="nb">len</span><span class="p">(</span><span class="n">cleaned_text</span><span class="p">[</span><span class="n">i</span><span class="p">]</span><span class="o">.</span><span class="n">split</span><span class="p">())</span> <span class="o">&lt;=</span> <span class="n">MAX_TEXT_LEN</span><span class="p">):</span>
        <span class="n">short_text</span><span class="o">.</span><span class="n">append</span><span class="p">(</span><span class="n">cleaned_text</span><span class="p">[</span><span class="n">i</span><span class="p">])</span>
        <span class="n">short_summary</span><span class="o">.</span><span class="n">append</span><span class="p">(</span><span class="n">cleaned_summary</span><span class="p">[</span><span class="n">i</span><span class="p">])</span>

<span class="c1"># change the dataframe</span>
<span class="n">final_dataframe</span> <span class="o">=</span> <span class="n">pd</span><span class="o">.</span><span class="n">DataFrame</span><span class="p">({</span><span class="s2">"text"</span><span class="p">:</span><span class="n">short_text</span><span class="p">,</span> <span class="s2">"summary"</span><span class="p">:</span><span class="n">short_summary</span><span class="p">})</span>

<span class="c1"># ADD SOSEQ &amp; EOSEQ Tokens to start and end of summary so model knows when to start generating and when to stop</span>
<span class="n">final_dataframe</span><span class="p">[</span><span class="s2">"summary"</span><span class="p">]</span> <span class="o">=</span> <span class="n">final_dataframe</span><span class="p">[</span><span class="s2">"summary"</span><span class="p">]</span><span class="o">.</span><span class="n">apply</span><span class="p">(</span><span class="k">lambda</span> <span class="n">x</span><span class="p">:</span> <span class="s2">"soseq "</span> <span class="o">+</span> <span class="n">x</span> <span class="o">+</span> <span class="s2">" eoseq"</span><span class="p">)</span>
<span class="n">final_dataframe</span><span class="o">.</span><span class="n">head</span><span class="p">(</span><span class="mi">10</span><span class="p">)</span>


<span class="c1"># define arrays for data </span>
<span class="n">texts</span> <span class="o">=</span> <span class="n">np</span><span class="o">.</span><span class="n">array</span><span class="p">(</span><span class="n">final_dataframe</span><span class="p">[</span><span class="s2">"text"</span><span class="p">])</span>
<span class="n">summaries</span> <span class="o">=</span> <span class="n">np</span><span class="o">.</span><span class="n">array</span><span class="p">(</span><span class="n">final_dataframe</span><span class="p">[</span><span class="s2">"summary"</span><span class="p">])</span>

<span class="c1"># divide train and validation</span>
<span class="n">x_train</span><span class="p">,</span> <span class="n">x_validation</span><span class="p">,</span> <span class="n">y_train</span><span class="p">,</span> <span class="n">y_validation</span> <span class="o">=</span> <span class="n">tts</span><span class="p">(</span><span class="n">texts</span><span class="p">,</span> <span class="n">summaries</span><span class="p">,</span> <span class="n">test_size</span><span class="o">=</span><span class="mf">0.10</span><span class="p">,</span> <span class="n">random_state</span><span class="o">=</span><span class="mi">0</span><span class="p">,</span> <span class="n">shuffle</span><span class="o">=</span><span class="kc">True</span><span class="p">)</span>


<span class="c1"># define the x tokenizer (texts)</span>
<span class="n">x_tokenizer</span> <span class="o">=</span> <span class="n">Tokenizer</span><span class="p">()</span>
<span class="n">x_tokenizer</span><span class="o">.</span><span class="n">fit_on_texts</span><span class="p">(</span><span class="nb">list</span><span class="p">(</span><span class="n">x_train</span><span class="p">))</span>

<span class="c1"># Define the y Tokenizer (summaries)</span>
<span class="n">y_tokenizer</span> <span class="o">=</span> <span class="n">Tokenizer</span><span class="p">()</span>
<span class="n">y_tokenizer</span><span class="o">.</span><span class="n">fit_on_texts</span><span class="p">(</span><span class="nb">list</span><span class="p">(</span><span class="n">y_train</span><span class="p">))</span>

<span class="c1">#convert text sequences into integer sequences</span>
